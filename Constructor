class employee:
    def __init__ (self,name,age,aim,dream):
        self.name=name
        self.age=age
        self.aim=aim
        self.dream=dream
    def employee_details(self):
        print("the Name of the Employee is",self.name)
        print("the Age of the Employee is",self.age)
        print("the Aim of the Employee is",self.aim)
        print("the Dream of the Employee is",self.dream)
        return ''
e1=employee("Hari",20,"Aim","Dream")
print(e1.employee_details())
